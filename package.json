{
  "name": "fragments",
  "private": true,
  "version": "0.10.7",
  "description": "Fragments back-end API",
  "scripts": {
    "test:watch": "jest -c jest.config.js --runInBand --watch --",
    "test": "jest -c jest.config.js --runInBand --",
    "coverage": "jest -c jest.config.js --runInBand --coverage",
    "lint": "eslint \"./src/**/*.js\" \"tests/**/*.js\"",
    "start": "node src/index.js",
    "dev": "LOG_LEVEL=debug nodemon ./src/index.js --watch src",
    "debug": "LOG_LEVEL=debug nodemon --inspect=0.0.0.0:9229 ./src/index.js --watch src",
    "test:integration": "hurl --test --glob 'tests/integration/**/*.hurl'",
    "hadolint": "hadolint Dockerfile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samartho4/fragments.git"
  },
  "author": "Samarth",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@orangeopensource/hurl": "^6.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "3.4.2",
    "resolve-cwd": "^3.0.0",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.782.0",
    "@aws-sdk/client-s3": "^3.772.0",
    "@aws-sdk/lib-dynamodb": "^3.782.0",
    "aws-jwt-verify": "^5.0.0",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "http-auth": "^4.2.0",
    "http-auth-passport": "^1.0.7",
    "js-yaml": "^4.1.0",
    "normalize-path": "^3.0.0",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pino": "^9.6.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "sharp": "^0.34.1",
    "sonic-boom": "^4.2.0",
    "stoppable": "^1.1.0"
  }
}
